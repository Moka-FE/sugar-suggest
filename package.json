{
	"name": "sugar-suggest",
	"displayName": "sugar-suggest",
	"description": "",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.39.0"
	},
	"publisher": "wYuanHpp",
	"repository": "git@github.com:wYuanHpp/sugar-suggest.git",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:stylus"
	],
  "main": "./extension.js",
	"contributes": {
    "configuration": {
      "type": "object",
      "title": "sugar-suggest",
      "properties": {
        "sugar-suggest.variableName": {
          "type": "string",
          "default": "sdf",
          "description": "sugar foundation 挂在的变量名"
        }, 
        "sugar-suggest.path": {
          "type": "string",
          "default": "@SDFoundation",
          "description": "自动引入sugar foundation的路径"
        },
        "sugar-suggest.variablePath": {
          "type": "string",
          "default": "~@SDVariable",
          "description": "自动引入sugar variable的路径"
        } 
      }
    }
	},
	"scripts": {
		"test": "node ./test/runTest.js"
  },
  "dependencies": {
		"lodash": "^4.17.15",
		"stylus": "^0.54.7"
  },
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
